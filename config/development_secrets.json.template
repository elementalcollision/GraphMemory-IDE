{
  "environment": "development",
  "api_keys": {
    "rotation_days": 180,
    "default_scopes": ["read", "write", "dashboard"],
    "rate_limit_per_minute": 100,
    "rate_limit_per_hour": 1000,
    "enable_auto_rotation": true,
    "max_keys_per_project": 15,
    "key_prefix": "gm_dev",
    "entropy_bits": 256
  },
  "database_credentials": {
    "rotation_days": 90,
    "encryption_algorithm": "aes-256-gcm",
    "enable_dynamic_secrets": false,
    "connection_pool_size": 5
  },
  "jwt": {
    "algorithm": "EdDSA",
    "key_rotation_days": 30,
    "max_key_versions": 3,
    "enable_hsm": false
  },
  "ssl_certificates": {
    "auto_renewal_days": 30,
    "certificate_authority": "development_ca",
    "enable_transparency_logging": false
  },
  "audit": {
    "log_level": "DEBUG",
    "retention_days": 30,
    "enable_real_time_alerts": false,
    "compliance_frameworks": ["soc2"]
  },
  "secrets_storage": {
    "backend": "filesystem",
    "encryption_at_rest": true,
    "backup_enabled": false,
    "compression_enabled": false
  },
  "environment_variables": {
    "DATABASE_URL": "postgresql://dev_user:YOUR_DEV_PASSWORD@localhost:5432/graphmemory_dev",
    "REDIS_URL": "redis://localhost:6379/0",
    "LOG_LEVEL": "DEBUG",
    "ENABLE_DEBUG_FEATURES": "true"
  }
} 